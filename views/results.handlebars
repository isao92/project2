<div class="row">
    <div class="col-8 offset-2">
        <h1>Hello {{user.email}}!</h1>
        <header>
            <h1 id="djr">Dream Job Results!<span class="badge badge-secondary"></span></h1>
        </header>
        <div class="container">
            <div class="results">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">

                            <div class="card-header">
                                Your Results:
                            </div>
                            <div class="card-body">
                                <div class="resultlist">

                                    {{#each jobs}}
                                    <div class="card" style="width: 18rem;">
                                        <div class="card-body">
                                            <h5 class="card-title">{{this.MatchedObjectDescriptor.PositionTitle}}</h5>
                                            <h6 class="card-subtitle mb-2 text-muted">{{this.MatchedObjectDescriptor.PositionLocationDisplay}}</h6>
                                            <p class="card-text"> {{this.MatchedObjectDescriptor.OrganizationName}}</p>
                                            <a href={{this.MatchedObjectDescriptor.ApplyURI}} class="card-link">"Click
                                                here to Apply!</a>
                                                <button type="button" class="btn btn-primary btn-sm" id="submitResult">Save Job To Profile</button>

                                        </div>
                                    </div>
                                    <br><br>
                                    {{!-- <h2>
                                        {{this.MatchedObjectDescriptor.PositionTitle}}
                                    </h2>

                                    <li>
                                        {{this.MatchedObjectDescriptor.PositionLocationDisplay}}
                                    </li>
                                    <li>
                                        {{this.MatchedObjectDescriptor.OrganizationName}}
                                    </li>

                                    <a href={{this.MatchedObjectDescriptor.ApplyURI}}>"Click the link to Apply!</a>
                                    --}}

                                    {{!-- {{this.MatchedObjectDescriptor.PositionSchedule.Name}} --}}
                                    {{!-- {{this.MatchedObjectDescriptor.PositionLocation.CountrySubDivisionCode}} --}}

                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12" id="force-height"></div>
                </div>
            </div>
        </div>